//Faça um procedimento que faça a leitura um vetor de 10 elementos inteiros e imprima somente os valores armazenados nos índices pares

#include <stdio.h>
#include<stdlib.h>
#include<math.h>

int main(void) {
  int vetor[10];
  int i=1;
  for(int i=1;i<=10;i++) {
    printf("Digite o número da posição %d:",i);
    scanf("%d",&vetor[i]); 
  }
  for(int i=1;i<=10;i++) {
    if(i%2==0){
      printf("O número na posição %d é: %.0d\n",i,vetor[i]);
    }

  }
  return 0;
}
